{
  "name": "moonview",
  "version": "0.0.1",
  "description": "A minimal writing application",
  "main": "main.js",
  "appname": "MoonView",
  "app-version": "0.0.1",
  "scripts": {
    "start": "electron main.js",
    "build-mac": "electron-packager . Moonview --platform=darwin --arch=x64 --icon='./_src/build-assets/mac-icon.icns' --out=dist --ignore='^/dist$' --overwrite",
    "build-win": "electron-packager . Moonview --platform=win32 --arch=x64 --icon='./_src/build-assets/win-icon.ico' --out=dist --ignore='^/dist$' --overwrite",
    "build-linux": "electron-packager . Moonview --platform=linux --arch=x64 --icon='./_src/build-assets/linux-icon.png' --out=dist --ignore='^/dist$' --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teesloane/moonview.git"
  },
  "author": "Tyler Sloane",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/teesloane/moonview/issues"
  },
  "homepage": "https://github.com/teesloane/moonview/blob/master/README.md",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "electron-packager": "^10.1.0",
    "electron-prebuilt": "^1.2.0",
    "eslint": "^2.11.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2"
  },
  "dependencies": {
    "file-url": "^1.1.0",
    "image-size": "^0.5.0",
    "marked": "^0.3.5"
  }
}
